        "globals": "^15.0.0",
        "html-webpack-plugin": "^5.6.0",
        "pre-commit": "^1.2.2",
        "prettier": "^3.2.5",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router": "^6.22.3",
